{
    "manifest_version" : 2
,   "name" : "__MSG_ext_title__"
,   "short_name" : "__MSG_ext_short_name__"
,   "version" : "0.1.2.1"
,   "description" : "__MSG_ext_description__"
,   "default_locale" : "ja"
,   "icons" : {
        "48" : "img/icon_48.png"
    ,   "96" : "img/icon_96.png"
    ,   "128" : "img/icon_128.png"
    }
,   "content_scripts" : [
        {
            "matches" : [ "http://twitter.com/*", "https://twitter.com/*", "https://pbs.twimg.com/media/*" ]
        ,   "js" : [ "js/init.js" ]
        ,   "run_at" : "document_start"
        }
    ,   {
            "matches" : [ "http://twitter.com/*", "https://twitter.com/*" ]
        ,   "js" : [ "js/twOpenOriginalImage.user.js" ]
        ,   "run_at" : "document_end"
        }
    ,   {
            "matches" : [ "https://pbs.twimg.com/media/*" ]
        ,   "js" : [ "js/twImageDownloadHelper.user.js" ]
        ,   "run_at" : "document_end"
        }
    ]
,   "background" : {
        "scripts" : [ "js/background.js" ]
    }
,   "options_page" : "html/options.html"
,   "browser_action" : {
        "default_icon" : "img/icon_48.png"
    ,   "default_title" : "__MSG_ext_title__"
    ,   "default_popup" : "html/options.html"
    }
}
